/* default.css */
body
{
	font: 12px "Lucida Grande", Tahoma, Arial, Helvetica, sans-serif;
	font-family: 'Helvetica Neue', Helvetica, Arial, sans-serif;
	padding: 0px;
	background-color: #F6F4F2;
	margin: 0px;
	overflow: hidden;
}

.header
{
	clear: both;
	background-color: #ccc;
	padding: 0px;
}

h1
{
	font-size: 150%;
	font-weight: bold;
	padding: 0;
	margin: 1em 0 0 .3em;
}

hr
{
	border: none 0;
	border-top: 1px solid #302F2D;
	height: 1px;
}

pre.code
{
	display: block;
	padding: 8px;
	border: 1px dashed #ccc;
}

a {
  text-decoration: none;
  color: #D0410F;
}

a:hover {
  color: #242220;
}


#index {
  left: 0px;
  margin-top: 0px;
  padding: 0px;
  top: 0px;
  position: fixed;
  width: 100%;
  height: 32px;
  overflow: hidden;
  color: #F6F4F2;
  z-index: 1;
  border-bottom: 1px solid grey;
  background-color: #F6F4F2;

  text-shadow: #222 0px -1px 1px;
  text-shadow: white 0px 1px 1px;
}

#index > .title {
  background: #515151 -webkit-gradient(linear, 0% 0%, 0% 100%, color-stop(0.2, #515151), color-stop(0.8, #302F2D));
  height: 27px;
  color: #F6F4F2;
  padding-top: 5px;
  padding-left: 8px;
  font-size: 18px;
}

#index > .title a {
  text-shadow: black 0px 1px 1px;
  color: orange
}

#index > .title > h2 {
  display: inline;
  margin: 0px;
  float: right;
  margin: 2px;
  margin-right: 10px;
  text-shadow: black 0px 1px 1px;
  font-size: 18px;
  font-weight: normal;
}

#index:hover {
  height: auto;
}

#scrollable
{
  position: fixed;
  top: 66px;
  left: 200px;
  right: 0px;
  bottom: 0px;

  overflow-y: scroll;
}

#content
{
  font-family: 'Helvetica Neue', Helvetica, Arial, sans-serif;
  color: #242220;
  
  padding-left: 15px;
  padding-right: 15px;
  font-size: 13px;
}

.classList
{
	list-style-type: none;
	padding: 0;
	margin: 8px 8px 5px 8px;
	font-family: arial, sans-serif;
	font-size: 1em;
	overflow: auto;
	font-size: 14px;
}

.classList li
{
	padding: 0;
	margin: 0 0 8px 8px;
	padding: 0;
	
	display: inline-block;
}

.classList li > a
{
	padding-left: 8px;
}

.classList li:before {
  content: '|';
  color: grey;
}
.classList li:first-child:before {
  content: '';
  color: grey;
}

.summaryTable { width: 100%; }

.classTitle
{
  position: fixed;
  top: 33px;
  left: 0px;
  right: 0px;
  height: 27px;
  color: #242220;
  text-shadow: white 0px 1px 1px;
  font-size: 18px;
  background-image: -webkit-gradient(linear, 0% 0%, 0% 100%, from(#F9F9F9), to(#CBCBCB));
  border-bottom: 1px solid gray;
  padding-top: 5px;
  text-align: center;
}

h2 { font-size: 110%; }
caption, div.sectionTitle
{
	background-color: #302F2D;
	color: #fff;
	font-size:130%;
	text-align: left;
	padding: 2px 6px 2px 6px;
	border: 1px #302F2D solid;
}

div.sectionTitle { margin-bottom: 8px; }
.summaryTable thead { display: none; }

.summaryTable td
{
	vertical-align: top;
	padding: 4px;
	border-bottom: 1px #302F2D solid;
	border-right: 1px #302F2D solid;
}

/*col#summaryAttributes {}*/
.summaryTable td.attributes
{
	border-left: 1px #302F2D solid;
	width: 140px;
	text-align: right;
}

td.attributes, .fixedFont
{
	line-height: 15px;
	color: #302F2D;
	font-family: "Courier New",Courier,monospace;
	font-size: 13px;
}

.summaryTable td.nameDescription
{
	text-align: left;
	font-size: 13px;
	line-height: 15px;
}

.summaryTable td.nameDescription, .description
{
	line-height: 15px;
	padding: 4px;
	padding-left: 4px;
}

#content > .description:first-child {
//  font-size: 1.1em;
  font-size: 13px;
  border-bottom: 1px solid grey;
  padding-bottom: 10px;
}

.summaryTable { margin-bottom: 8px; }

ul.inheritsList
{
	list-style: square;
	margin-left: 20px;
	padding-left: 0;
}

.detailList {
	margin-left: 20px; 
	line-height: 15px;
}
.detailList dt { margin-left: 20px; }

.detailList .heading
{
	font-weight: bold;
	padding-bottom: 6px;
	margin-left: 0;
}

.light, td.attributes, .light a:link, .light a:visited
{
	color: #777;
	font-style: italic;
}

#index_tree {
  position: fixed;
  top: 66px;
  bottom: 0px;
  width: 200px;
  height: 1500px;
  border: none;
  border-right: 1px grey solid;
  background-color: #ECEAE7;
}

.index #index_tree, .index #scrollable {
  top: 33px;
}

.index #content .classTitle {
  margin: 0px;
  top: 0px;
}

.inheritsList > dd {
  padding-bottom: 5px;
}

hr:last-child {
  border:none;
}

#content .class_info {
  width: 200px;
  height: 100px;
  display: inline-block;
  padding: 0px 10px;
  overflow: hidden;
}

.fineprint {
	text-align: right;
	font-size: 10px;

  position: fixed;
  bottom: 0px;
  left: 0px;
  width: 190px;
  z-index: 10;
  
  background-color: #302F2D;
  color: #F6F4F2;
  padding: 5px;
}

/********************************************************************
              Main Scrollbar configuration
********************************************************************/

::-webkit-scrollbar {
  width: 10px;
  height: 10px;
  background-color: transparent;
}

::-webkit-scrollbar-corner {
/*  background-color: transparent; */
  background-color: white;
}

::-webkit-scrollbar-track-piece:disabled {
  display: none !important;
}

::-webkit-scrollbar-button:disabled {
  display: none !important;
}
 
::-webkit-scrollbar-track:disabled {
  margin: 6px;
}

/* Vertical Scrollbar Styles */

::-webkit-scrollbar-thumb:vertical {
  min-height: 20px;
//  border-top: 1px solid #fff;  
  background: #ccc -webkit-gradient(linear, 0% 0%, 100% 0% , from(rgb(240, 240, 240)), to(rgb(210, 210, 210)));  
  -webkit-border-radius: 1ex;  
  -webkit-box-shadow: 0 1px 3px rgba(0, 0, 0, .4);  
}
 
::-webkit-scrollbar-thumb:vertical:hover {
  background: #ccc -webkit-gradient(linear, 0% 0%, 100% 0% , from(rgb(140, 140, 140)), to(rgb(110, 110, 110)));  
}
 
::-webkit-scrollbar-thumb:vertical:active {
  background: #ccc -webkit-gradient(linear, 0% 0%, 100% 0% , from(rgb(140, 140, 140)), to(rgb(110, 110, 110)));  
}
 
::-webkit-scrollbar-thumb:vertical:window-inactive {
  background: transparent;  
}
